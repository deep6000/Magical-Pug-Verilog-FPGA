	component VGA_Subsystem is
		port (
			clk_clk         : in  std_logic                    := 'X';             -- clk
			digit_0_digit_0 : out std_logic_vector(7 downto 0);                    -- digit_0
			digit_1_digit_1 : out std_logic_vector(7 downto 0);                    -- digit_1
			digit_2_digit_2 : out std_logic_vector(7 downto 0);                    -- digit_2
			keys_keys       : in  std_logic_vector(3 downto 0) := (others => 'X'); -- keys
			reset_reset_n   : in  std_logic                    := 'X';             -- reset_n
			score_score     : out std_logic_vector(7 downto 0);                    -- score
			switch_switch   : in  std_logic                    := 'X';             -- switch
			vga_CLK         : out std_logic;                                       -- CLK
			vga_HS          : out std_logic;                                       -- HS
			vga_VS          : out std_logic;                                       -- VS
			vga_BLANK       : out std_logic;                                       -- BLANK
			vga_SYNC        : out std_logic;                                       -- SYNC
			vga_R           : out std_logic_vector(7 downto 0);                    -- R
			vga_G           : out std_logic_vector(7 downto 0);                    -- G
			vga_B           : out std_logic_vector(7 downto 0)                     -- B
		);
	end component VGA_Subsystem;

